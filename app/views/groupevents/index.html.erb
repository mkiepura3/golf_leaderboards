<div class="page-header">
  <h1>
    Groupevents
    <a href="/groupevents/new" class="btn btn-lg btn-success">New Groupevent</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Eventname</th>
        <th>Course</th>
        <th>Date</th>
        <th>Actions</th>
      </tr>

      <% @groupevents.each do |groupevent| %>
      <tr>
        <td><%= groupevent.eventname %></td>
        <td><%= groupevent.course_id %></td>
        <td><%= groupevent.date %></td>
        <td>
          <a href="/groupevents/<%= groupevent.id %>" class="btn btn-primary">Show</a>
          <a href="/groupevents/<%= groupevent.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_groupevent/<%= groupevent.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
